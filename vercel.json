{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node", "config": { "includeFiles": [
        "node_modules/.prisma/client/libquery_engine-*",
        "node_modules/@prisma/client/**/*",
        "src/views/**/*",
        "src/public/**/*"
      ] } }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
